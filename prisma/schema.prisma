
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.sqlite"
}

model User {
  id Int @id @default(autoincrement())
  nome String 
  email String @unique  
  senha String
  dataNascimento DateTime
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}